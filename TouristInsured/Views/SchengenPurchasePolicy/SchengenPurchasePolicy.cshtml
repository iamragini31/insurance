    function GetState() {

        var ddlBillingCountry = $("#ddlBillingCountry").find('option:selected').val();

        if (ddlBillingCountry == "US") {

            var StateList = '<option value=0>Select State</option>';
            $.ajax({
                url: "/PurchasePolicy/GetStatedetails/",
