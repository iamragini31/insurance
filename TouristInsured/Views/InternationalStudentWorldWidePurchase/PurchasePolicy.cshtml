    function GetState() {

        var ddlBillingCountry = $("#ddlBillingCountry").find('option:selected').val();

        if (ddlBillingCountry == "US") {

            var StateList = '<option value=0>Select State</option>';
            $.ajax({
                url: "/PurchasePolicy/GetStatedetails/",
                type: "GET",
                contentType: "application/json",
                dataType: "json",


                success: function (data) {
                    debugger
                    if (data.length > 0) {
                        $.each(data, function (i, item) {

                            StateList = StateList + '<option value=' + item.Statecode + '>' + item.statename + '</option>';
                        });

                        $('#ddlBillingState').html(StateList);
                        var ddlState = document.getElementById("ddlBillingState");
                        ddlState.style.display = "block";
